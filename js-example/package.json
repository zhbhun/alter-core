    },
    "homepage": "https://alter.xyz/",
    "scripts": {
        "renderAvatar": "webpack serve --host 0.0.0.0 --config webpack.config.dev.js --env entry=./src/renderAvatar.ts",
        "cursorFollow": "webpack serve --host 0.0.0.0 --config webpack.config.dev.js --env entry=./src/cursorFollow.ts",
        "faceTracking": "webpack serve --https --host 0.0.0.0 --config webpack.config.dev.js --env entry=./src/faceTracking.ts",
        "limitedMovement": "webpack serve --https --host 0.0.0.0 --config webpack.config.dev.js --env entry=./src/limitedMovement.ts",
        "deSerialization": "webpack serve --https --host 0.0.0.0 --config webpack.config.dev.js --env entry=./src/deSerialization.ts",
        "build": "webpack --config webpack.config.prod.js --env entry=./src/renderAvatar.ts"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^10.2.4",
        "html-webpack-plugin": "^5.5.0",
